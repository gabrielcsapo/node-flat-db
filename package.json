{
  "name": "node-flat-db",
  "version": "0.1.0",
  "description": "JSON database for Node and the browser powered by lodash",
  "keywords": [
    "flat",
    "file",
    "local",
    "database",
    "JSON",
    "lo-dash",
    "lodash",
    "underscore",
    "localStorage",
    "embed",
    "embeddable",
    "extendable"
  ],
  "main": ".",
  "scripts": {
    "test": "babel-node test/all | tap-spec",
    "prepublish": "npm run build",
    "precommit": "npm test",
    "build": "npm run umd && npm run min",
    "umd": "webpack src/dist.js dist/flat-db.js --output-library flat-db",
    "min": "webpack -p src/dist.js dist/flat-db.min.js --output-library flat-db"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gabrielcsapo/node-flat-db.git"
  },
  "author": "Gabriel J. Csapo <gabecsapo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielcsapo/node-flat-db/issues"
  },
  "homepage": "https://github.com/gabrielcsapo/node-flat-db",
  "dependencies": {
    "graceful-fs": "^4.1.3",
    "is-promise": "^2.1.0",
    "json-parse-helpfulerror": "^1.0.3",
    "lodash": "^4.0.0",
    "steno": "^0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-3": "^6.3.13",
    "husky": "^0.11.0",
    "sinon": "^1.17.2",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "tempfile": "^1.1.1",
    "underscore-db": "^0.10.0",
    "webpack": "^1.12.13"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
